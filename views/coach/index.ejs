<div class="container">
  <h3>Users</h3>
  <table class='table'>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Email</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>


    <% _.each(coaches, function(coach) { %>
    <tr data-id="<%= user.id %>" data-model="user">
      <td><%= coach.id %></td>
      <td><%= coach.firstname %></td>
      <td><%= coach.lastname %></td>
      <td><%= coach.email %></td>
      <td><a href="/coach/show/<%= coach.id %>" class="btn btn-sm btn-primary">Show</a></td>
      <td><a href="/coach/edit/<%= coach.id %>" class="btn btn-sm btn-warning">Edit</a></td>

      <td><form action="/coach/destroy/<%= coach.id %>" method="POST">
          <input type="hidden" name="_method" value="delete"/>
          <input type="submit" class="btn btn-sm btn-danger" value="Delete"/>
          <input type="hidden" name="_csrf" value="<%= _csrf %>" />
        </form>
    </tr>

    <% }) %>
  </table>
</div>
